<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration  
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"  
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

	<context id="mysql_tables" targetRuntime="MyBatis3">

		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
						connectionURL="jdbc:mysql://127.0.0.1:3306/sy"
						userId="root"
						password="123456" />
		<!-- 指定生成的类型为java类型，避免数据库中number等类型字段 -->
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- 生成model模型，对应的包，存放位置可以指定具体的路径,如/ProjectName/src，也可以使用MAVEN来自动生成 -->
		<javaModelGenerator targetPackage="com.yf.base.model.emergency"
			targetProject="src\main\java">
			<property name="enableSubPackages" value="true" />
		</javaModelGenerator>

		<!--对应的xml mapper文件 -->
		<sqlMapGenerator targetPackage="/mybatis/mysql/emergency/"
			targetProject="src\main\resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<!-- 对应的dao接口 -->
		<javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.yf.base.dao.emergency" targetProject="src\main\java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<table tableName="emergency_event_process" domainObjectName="EmergencyEventProcess" schema="sy"
			   enableSelectByExample="false" enableDeleteByExample="false"
			   enableCountByExample="false" enableUpdateByExample="false" />

		<table tableName="resource_area_data" domainObjectName="ResourceAreaData" schema="sy"
			   enableSelectByExample="false" enableDeleteByExample="false"
			   enableCountByExample="false" enableUpdateByExample="false" />
	</context>
</generatorConfiguration>  
